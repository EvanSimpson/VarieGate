{
	"manifest_version": 2,

	"name": "VarieGate",
	"description": "A way to export browsing history in Chrome",
	"version": "0.2",

	"permissions": [
   		"tabs"
	],

	"background": {
		"scripts" : ["background.js"]
	},

	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["jquery-2.0.3.min.js","record.js"],
			"run_at": "document_end"
		}
	]

}